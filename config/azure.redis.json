{
  "connection": {
    "host": "YOUR_CACHE_NAME.redis.cache.windows.net",
    "port": 6380,
    "username": "YOUR_CACHE_NAME",
    "password": "YOUR_ACCESS_KEY",
    "tls": {
      "enabled": true,
      "servername": "YOUR_CACHE_NAME.redis.cache.windows.net"
    },
    "connectionString": "rediss://YOUR_CACHE_NAME:YOUR_ACCESS_KEY@YOUR_CACHE_NAME.redis.cache.windows.net:6380?ssl=true"
  },
  
  "retry": {
    "maxRetries": 5,
    "initialDelay": 200,
    "maxDelay": 2000,
    "factor": 2,
    "jitter": 0.1
  },
  
  "cluster": {
    "enabled": false,
    "nodes": [
      { "host": "YOUR_CACHE_NAME.redis.cache.windows.net", "port": 15000 },
      { "host": "YOUR_CACHE_NAME.redis.cache.windows.net", "port": 15001 }
    ],
    "options": {
      "scaleReads": "slave",
      "enableReadyCheck": true,
      "retryDelayOnFailover": 100,
      "retryDelayOnClusterDown": 100,
      "retryDelayOnTryAgain": 100
    }
  },
  
  "cache": {
    "defaultTtl": 3600,
    "maxKeys": 10000,
    "checkPeriod": 60,
    "autoPipeline": true,
    "enableOfflineQueue": true,
    "maxRetriesPerRequest": 3
  },
  
  "metrics": {
    "enabled": true,
    "prefix": "azure.redis.",
    "collectInterval": 30000,
    "azureMonitor": {
      "enabled": true,
      "resourceId": "/subscriptions/YOUR_SUBSCRIPTION_ID/resourceGroups/YOUR_RESOURCE_GROUP/providers/Microsoft.Cache/redis/YOUR_CACHE_NAME"
    }
  },
  
  "azure": {
    "cacheName": "YOUR_CACHE_NAME",
    "skuName": "Standard_C1",
    "shardCount": 0,
    "enableNonSslPort": false,
    "minimumTlsVersion": "1.2",
    "publicNetworkAccess": "Enabled",
    "subnetId": "",
    "staticIp": "",
    "zones": ["1", "2", "3"],
    "replicasPerMaster": 1,
    "tenantSettings": {
      "maxmemory-policy": "allkeys-lru",
      "notify-keyspace-events": "KExg"
    }
  }
}
