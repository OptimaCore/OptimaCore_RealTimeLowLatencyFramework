{
  "name": "optimacore_realtimelowlatencyframework",
  "version": "0.1.0",
  "description": "OptimaCore Real-Time Low Latency Framework - High-performance, scalable infrastructure for low-latency applications",
  "main": "index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "build": "npm run clean && tsc",
    "build:frontend": "next build frontend",
    "start:frontend": "next start -p 3000",
    "dev:frontend": "next dev -p 3000",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:azure": "node scripts/test-azure-connection.js",
    "test:azure:cluster": "node scripts/test-azure-connection.js --cluster",
    "test:cache": "node scripts/test-cache-hit.js",
    "test:docker": "jest tests/docker.test.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "prepare": "husky install",
    "bench": "node benchmarks/run.js",
    "redis:test": "node services/redis/test.js",
    "docker:build": "node scripts/build-docker.js",
    "docker:push": "node scripts/push-images.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart",
    "gateway:start": "node services/api-gateway/index.js",
    "gateway:dev": "nodemon services/api-gateway/index.js",
    "test:gateway": "jest tests/api-gateway.test.js --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OptimaCore/OptimaCore_RealTimeLowLatencyFramework.git"
  },
  "keywords": [
    "low-latency",
    "real-time",
    "high-performance",
    "scalable",
    "infrastructure"
  ],
  "author": "OptimaCore Team",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/OptimaCore/OptimaCore_RealTimeLowLatencyFramework/issues"
  },
  "homepage": "https://github.com/OptimaCore/OptimaCore_RealTimeLowLatencyFramework#readme",
  "devDependencies": {
    "@types/socket.io": "^3.0.2",
    "@types/uuid": "^9.0.0",
    "@types/express": "^4.17.17",
    "@types/supertest": "^6.0.2",
    "supertest": "^6.3.3",
    "@faker-js/faker": "^8.0.2",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^16.1.6",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@azure/web-pubsub": "^1.1.0",
    "@azure/web-pubsub-socket.io": "^1.0.0",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "uuid": "^9.0.0",
    "express-rate-limit": "^6.9.0",
    "http-proxy-middleware": "^2.0.6",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1",
    "rate-limiter-flexible": "^2.4.1",
    "@azure/identity": "^4.0.0",
    "@azure/monitor-query": "^1.2.0",
    "ajv": "^8.17.1",
    "async-retry": "^1.3.3",
    "chai": "^6.0.1",
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "ioredis": "^5.3.2",
    "pg": "^8.16.3",
    "pg-mock": "^1.0.1",
    "table": "^6.8.1",
    "uuid": "^9.0.0",
    "yaml": "^2.8.1"
  }
}
